  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.line",
        "comment.block",
        "comment.line.scss",
        "comment.line.double-slash.js",
        "comment.line.double-slash.ts",
        "comment.block.js",
        "comment.block.ts",
        "punctuation.definition.comment.scss",
        "punctuation.definition.comment.js",
        "punctuation.definition.comment.ts",
        "punctuation.definition.comment.html",
        "comment.block.html"
      ],
      "settings": {
        "foreground": "#6D6F7C",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords & Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator",
        "keyword.other",
        "storage"
      ],
      "settings": {
        "foreground": "#FF90EE",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Control Keywords",
      "scope": [
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.flow",
        "keyword.control.return",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#FF90EE",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Variables & Parameters",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.parameter.js",
        "variable.parameter.ts",
        "variable.parameter.function",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#E1F0FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function Names",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic",
        "variable.function",
        "entity.name.method"
      ],
      "settings": {
        "foreground": "#C9ECFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class Names & Types",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#CAC9FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.single",
        "string.quoted.double",
        "string.template"
      ],
      "settings": {
        "foreground": "#C9ECFF"
      }
    },
    {
      "name": "Template Literals & Interpolation",
      "scope": [
        "string.template",
        "string.quoted.template",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#96F9FF"
      }
    },
    {
      "name": "Numbers & Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FF90EE"
      }
    },
    {
      "name": "Boolean & Null",
      "scope": [
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#5738FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp",
        "string.regexp.character-class",
        "string.regexp.anchor",
        "string.regexp.quantifier"
      ],
      "settings": {
        "foreground": "#CAC9FF"
      }
    },
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#D1FFFD"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.html",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#E1F0FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#FF90EE"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.sass",
        "support.type.property-name.less"
      ],
      "settings": {
        "foreground": "#D1FFFD"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.font-name.css",
        "support.constant.media.css"
      ],
      "settings": {
        "foreground": "#C9ECFF"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.pseudo-class.css"
      ],
      "settings": {
        "foreground": "#96F9FF"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "string.quoted.double.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#E1F0FF"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "markup.heading.markdown",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#D1FFFD",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#FF90EE"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link.markdown",
        "string.other.link.title.markdown",
        "string.other.link.description.title.markdown"
      ],
      "settings": {
        "foreground": "#96F9FF",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Python Decorators",
      "scope": [
        "entity.name.function.decorator.python",
        "punctuation.definition.decorator.python"
      ],
      "settings": {
        "foreground": "#FF90EE",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python Self",
      "scope": [
        "variable.language.special.self.python",
        "variable.parameter.function.language.special.self.python"
      ],
      "settings": {
        "foreground": "#CAC9FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JavaScript/TypeScript Imports",
      "scope": [
        "variable.other.readwrite.alias.js",
        "variable.other.readwrite.alias.ts",
        "meta.import.js",
        "meta.import.ts"
      ],
      "settings": {
        "foreground": "#FF7676"
      }
    },
    {
      "name": "TypeScript Types",
      "scope": [
        "entity.name.type.ts",
        "entity.name.type.tsx",
        "support.type.primitive.ts",
        "support.type.primitive.tsx"
      ],
      "settings": {
        "foreground": "#CAC9FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "React JSX Component Names",
      "scope": [
        "support.class.component.jsx",
        "support.class.component.tsx",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#96F9FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition"
      ],
      "settings": {
        "foreground": "#A7A8AF"
      }
    },
    {
      "name": "Brackets & Braces",
      "scope": [
        "punctuation.section.brackets",
        "punctuation.section.braces",
        "punctuation.section.parens",
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly"
      ],
      "settings": {
        "foreground": "#E1F0FF"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.bitwise"
      ],
      "settings": {
        "foreground": "#FF90EE"
      }
    },
    {
      "name": "Invalid & Deprecated",
      "scope": [
        "invalid",
        "invalid.illegal",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FF7676",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Git Diff Added",
      "scope": [
        "markup.inserted",
        "markup.inserted.git_gutter"
      ],
      "settings": {
        "foreground": "#5738FF"
      }
    },
    {
      "name": "Git Diff Removed",
      "scope": [
        "markup.deleted",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#FF7676"
      }
    },
    {
      "name": "Git Diff Changed",
      "scope": [
        "markup.changed",
        "markup.changed.git_gutter"
      ],
      "settings": {
        "foreground": "#FF90EE"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#CAC9FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "URLs and Links",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Console & Debug",
      "scope": [
        "support.function.console",
        "entity.name.function.console"
      ],
      "settings": {
        "foreground": "#96F9FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Log Tokens",
      "scope": "token.info-token",
      "settings": {
        "foreground": "#96F9FF"
      }
    },
    {
      "name": "Warning Tokens",
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#FF90EE"
      }
    },
    {
      "name": "Error Tokens",
      "scope": "token.error-token",
      "settings": {
        "foreground": "#FF7676"
      }
    },
    {
      "name": "Debug Tokens",
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#CAC9FF"
      }
    },
    {
      "name": "Python Keywords",
      "scope": [
        "keyword.control.import.python",
        "keyword.control.flow.python",
        "keyword.operator.logical.python",
        "keyword.control.exception.python"
      ],
      "settings": {
        "foreground": "#FF90EE",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Python Built-ins",
      "scope": [
        "support.function.builtin.python",
        "support.type.python",
        "constant.language.python"
      ],
      "settings": {
        "foreground": "#CAC9FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python Magic Methods",
      "scope": [
        "support.function.magic.python",
        "variable.language.special.self.python",
        "variable.parameter.function.language.special.self.python",
        "variable.parameter.function.language.special.cls.python"
      ],
      "settings": {
        "foreground": "#96F9FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python Decorators",
      "scope": [
        "entity.name.function.decorator.python",
        "punctuation.definition.decorator.python",
        "meta.function.decorator.python"
      ],
      "settings": {
        "foreground": "#FF90EE",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python Strings and F-strings",
      "scope": [
        "string.quoted.single.python",
        "string.quoted.double.python",
        "string.quoted.triple.python",
        "meta.fstring.python",
        "string.interpolated.python"
      ],
      "settings": {
        "foreground": "#C9ECFF"
      }
    },
    {
      "name": "Python F-string Expressions",
      "scope": [
        "meta.fstring.python meta.interpolation.python",
        "punctuation.definition.interpolation.begin.python",
        "punctuation.definition.interpolation.end.python"
      ],
      "settings": {
        "foreground": "#96F9FF"
      }
